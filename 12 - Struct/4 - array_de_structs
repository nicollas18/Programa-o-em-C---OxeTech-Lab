#include <stdio.h>
#include <string.h>

struct Aluno {
  char nome[50];
  int idade;
  float nota;
};

int main() {

  struct Aluno turma[3] = {{"Jo√£o", 14, 7.5}, {"Clara", 14, 7.8}, {"Paulo", 14, 8.7}};

  for (int i = 0; i < 3; i++) {
    printf("%s\n", turma[i].nome);
    printf("%d\n", turma[i].idade);
    printf("%.2f\n", turma[i].nota);
  }

  for (int i = 0; i < 2; i++) {
    scanf("%50[^\n]", turma[i].nome);
    scanf("%d", &turma[i].idade);  
    scanf("%f", &turma[i].nota);
    getchar();
  }

 return 0;

}